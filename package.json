{
  "name": "celoterminal",
  "version": "1.0.14",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/zviadm/celoterminal"
  },
  "author": "Zviad Metreveli",
  "description": "Celo Terminal: Manage accounts and interact with the decentralized apps on Celo platform.",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "dev:baklava": "env-cmd -f env.dev.baklava electron-webpack dev",
    "dev:mainnet": "env-cmd -f env.dev.mainnet electron-webpack dev",
    "dev:fresh": "./env.dev.fresh.sh",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "lint": "yarn eslint src/",
    "test": "ELECTRON_RUN_AS_NODE=true yarn electron ./node_modules/.bin/jest"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "@material-ui/core",
      "@material-ui/lab",
      "@material-ui/icons"
    ],
    "renderer": {
      "webpackConfig": "webpack.additions.js"
    }
  },
  "dependencies": {
    "@celo/connect": "^1.0.1",
    "@celo/contractkit": "^1.0.1",
    "@celo/wallet-ledger": "^1.0.1",
    "@celo/wallet-local": "^1.0.1",
    "@fontsource/roboto": "^4.1.0",
    "@ledgerhq/electron-updater": "^4.2.2",
    "@ledgerhq/hw-app-eth": "^5.11.0",
    "@ledgerhq/hw-transport-node-hid-noevents": "^5.40.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "bignumber.js": "^9.0.1",
    "electron-log": "^4.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-support": "^0.5.16",
    "web3": "1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@svgr/webpack": "^5.5.0",
    "@types/better-sqlite3": "^5.4.1",
    "@types/jest": "^26.0.20",
    "@types/node-hid": "^1.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "css-loader": "^5.0.1",
    "electron": "11.2.1",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "node-sass": "4.14.1",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "9.0.3",
    "style-loader": "1.3.0",
    "tmp": "^0.2.1",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "webpack": "~4.42.1"
  }
}
